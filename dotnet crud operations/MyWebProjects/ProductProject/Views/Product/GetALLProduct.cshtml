@using BOL;
@{
    ViewData["Title"] = "Product Catalog";
}
<div align = "center" >
    @{
        List<Product> p_arr= ViewData["Products"] as List<Product>;
        foreach(Product p in p_arr){
           <div id="d2">
                <p id="para">Product ID : @p.P_ID</p>
                <p id="para">Product Desc : @p.P_Desc</p>
                <hr/>
            </div>
        }
    }
</div>